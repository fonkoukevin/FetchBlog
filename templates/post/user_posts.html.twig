{# post/user_posts.html.twig #}

{% extends 'base.html.twig' %}

{% block body %}
    <h1>Mes Posts</h1>
    <div class="posts">
        {% for post in posts %}
            <div class="post">
                <h2>{{ post.title }}</h2>
                <p>{{ post.content }}</p>
            </div>
        {% endfor %}
    </div>
{% endblock %}
